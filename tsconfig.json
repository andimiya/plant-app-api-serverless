{
  "compilerOptions": {
    "target": "ESNEXT",
    "module": "commonjs",
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "lib": ["es6", "dom"],
  "include": ["src/**/*", "app.local.ts"],
  "exclude": ["node_modules"]
}
